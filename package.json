    "drizzle-orm": "^0.44.7",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.13.0",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
